# Azure SQL Database Connection
# Replace with your actual server, database, and authentication details
SQL_CONNECTION_STRING=Driver="{ODBC Driver 18 for SQL Server};Server=tcp:YOUR-SERVER.database.windows.net,1433;Database=YOUR-DATABASE;Encrypt=yes;TrustServerCertificate=no;Connection Timeout=30;"

# Azure Blob Storage
# Replace with your storage account name and key
BLOB_CONNECTION_STRING="DefaultEndpointsProtocol=https;AccountName=YOUR-STORAGE-ACCOUNT;AccountKey=YOUR-STORAGE-KEY;EndpointSuffix=core.windows.net"

# Azure Blob Storage Containers
BLOB_CONTAINER_TEMPLATES=templates
BLOB_CONTAINER_OUTPUT=output

# Azure OpenAI
# Replace with your Azure OpenAI endpoint, API key, and deployment name
AOAI_ENDPOINT=https://YOUR-AOAI-RESOURCE.openai.azure.com/
AOAI_API_KEY=YOUR-OPENAI-API-KEY
AOAI_DEPLOYMENT=gpt-4o
OPENAI_API_VERSION=2024-12-01-preview
